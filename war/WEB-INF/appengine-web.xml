<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

  <application>winerecord</application>
  <version>1</version>

  <static-files>
    <include path="/**.css" />
    <include path="/**.ico" />
    <include path="/**.png" />
    <include path="/**.jpg" />
    <include path="/**.html" />
    <include path="/**.js" />
    <exclude path="/**.arc" />
  </static-files>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>

</appengine-web-app>
